{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Event",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "flyer_image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "title": {
      "type": "string",
      "required": true
    },
    "start_date_time": {
      "type": "datetime",
      "required": true
    },
    "end_date_time": {
      "type": "datetime",
      "required": true
    },
    "location": {
      "type": "string",
      "required": true
    },
    "seat_capacity": {
      "type": "integer",
      "required": false
    },
    "description": {
      "type": "customField",
      "options": {
        "output": "HTML",
        "preset": "rich"
      },
      "required": true,
      "customField": "plugin::ckeditor.CKEditor"
    },
    "event_type": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::event-type.event-type",
      "inversedBy": "events"
    }
  }
