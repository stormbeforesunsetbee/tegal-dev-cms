{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Event",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "flyer_image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "title": {
      "type": "string",
      "required": true
    },
    "start_date_time": {
      "type": "datetime",
      "required": true
    },
    "end_date_time": {
      "type": "datetime",
      "required": true
    },
    "location": {
      "type": "string",
      "required": true
    },
    "max_rsvp": {
      "type": "integer",
      "required": true,
      "default": 20
    },
    "description": {
      "type": "customField",
      "options": {
        "output": "HTML",
        "preset": "rich"
      },
      "required": true,
      "customField": "plugin::ckeditor.CKEditor"
    },
    "event_category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::event-category.event-category",
      "inversedBy": "events"
    },
    "total_rsvp": {
      "type": "integer",
      "required": true,
      "default": 0
    },
    "gmap_location_link": {
      "type": "string",
      "required": true
    },
    "xp_point": {
      "type": "integer",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "commitment_fee": {
      "type": "integer",
      "required": true,
      "default": 0
    },
    "rsvps": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::rsvp.rsvp",
      "mappedBy": "event"
    },
    "hackalearn_participants": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::hackalearn-participant.hackalearn-participant",
      "mappedBy": "event"
    },
    "event_questions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::event-question.event-question",
      "mappedBy": "event"
    },
    "code": {
      "type": "string",
      "required": true
    },
    "is_rsvp_closed": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "event_challenge": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::event-challenge.event-challenge"
    }
  }
}
